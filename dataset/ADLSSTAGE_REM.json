{
	"name": "ADLSSTAGE_REM",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "AzureBlobFSFile",
		"typeProperties": {
			"fileName": {
				"value": "@concat(item().SourceObjectName,'.txt')",
				"type": "Expression"
			},
			"folderPath": {
